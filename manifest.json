{
  "manifest_version": 2,

  "name": "NSA Goggles",
  "description": "See the web the way the NSA would. Highlights stuff that the NSA finds questionable.",
  "version": "1.0",
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
        "css": ["style.css"],
        "js": [
            "blacklist.js",
            "jquery.js",
            "highlight.js",
            "goggles.js"
        ]
      }],
      "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
  ]
}
